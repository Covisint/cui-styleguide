<!DOCTYPE html>
<html class="no-js" lang="en">
<head>
  <meta charset="utf-8">
  <title>{{options.title}}</title>

  <meta name="description" content="">
  <meta name="generator" content="kss-node">
  <meta name="viewport" content="width=device-width">

  <link href="//cloud.webtype.com/css/4122e018-fff4-4096-9642-2c791659a817.css" rel="stylesheet" type="text/css" />
  <link rel="stylesheet" href="kss-assets/kss.css">
  <link rel="stylesheet" href="aoc-styleguide-styles.css">
  {{{styles}}}
</head>
<body id="cui-styleguide__page-container">

<div class="cui-wrapper v2">
  <div class="snap-drawers">
    <div class="snap-drawer">
      <header class="kss-header">
        <h1 class="kss-doc-title">{{options.title}}</h1>
      </header>
      <nav class="kss-nav">
        <ul class="kss-nav__menu">
          <li class="kss-nav__menu-item">
            <a class="kss-nav__menu-link" href="./">
              <span class="kss-nav__ref">0</span
              ><span class="kss-nav__name">Overview</span>
            </a>
          </li>
        {{#each menu}}
          <li class="kss-nav__menu-item">
            <a class="kss-nav__menu-link" href="section-{{referenceURI}}.html">
              <span class="kss-nav__ref">{{referenceNumber}}</span><span class="kss-nav__name">{{header}}</span>
            </a>
            {{#if isActive}}
              {{#if children}}
                <ul class="kss-nav__menu-child">
                {{#each children}}
                  <li class="kss-nav__menu-item">
                    <a class="kss-nav__menu-link" href="section-{{../referenceURI}}.html#kssref-{{referenceURI}}">
                      <span class="kss-nav__ref {{#if isGrandChild}}kss-nav__ref-child{{/if}}">{{referenceNumber}}</span
                      ><span class="kss-nav__name">{{header}}</span>
                    </a>
                  </li>
                {{/each}}
                </ul>
              {{/if}}
            {{/if}}
          </li>
        {{/each}}
        </ul>
      </nav>
    </div>
  </div>
  <snap-content class="snap-content">

    <div class="cui-menu__content-wrapper">
      <div class="cui-menu__toggle-button--desktop cui-menu__toggle-button-container active">
        <button class="cui-menu__toggle-button"></button>
      </div>
      <header class="cui-header">
        <div class="cui-header__svg-logo-container">
          <svg class="cui-header__svg-logo" viewBox="0 0 640 124"><use xlink:href="node_modules/cui-icons/dist/logos/logos-out.svg#covisint-logo"></use></svg>
        </div>
        <div class="cui-header__top">
          <div class="cui-menu__toggle-button-container cui-menu__toggle-button--mobile" style="z-index: 10;" snap-toggle="left">
            <button class="cui-menu__toggle-button"></button>
          </div>
      </header>

      <div class="cui-styleguide__main-container">

        <nav class="cui-off-canvas cui-desktop-menu">
          <ul class="cui-off-canvas__list">
            <li class="cui-off-canvas__section">
              <a class="cui-off-canvas__section-link" href="./">
                <span class="cui-styleguide__list-item-name">Overview</span>
              </a>
            </li>
          {{#each menu}}
            <li class="cui-off-canvas__section cui-off-canvas__section--active">
              <a class="cui-off-canvas__section-link" href="section-{{referenceURI}}.html">
                <span class="cui-styleguide__nav__name">{{header}}</span>
              </a>
            {{#if isActive}}
              {{#if children}}
                <ul class="cui-off-canvas__sub-section-list">
                  {{#each children}}
                    <li class="cui-off-canvas__sub-section">
                      <a href="section-{{../../referenceURI}}.html#kssref-{{referenceURI}}">
                        <span class="cui-styleguide__nav__name">{{header}}</span>
                      </a>
                    </li>
                  {{/each}}
                </ul>
              {{/if}}
            {{/if}}
            </li>
          {{/each}}
          </ul>
        </nav>

        <article role="main" class="cui-styleguide__main">
        {{#if homepage}}
          <div id="kssref-0" class="cui-tile cui-styleguide__section cui-styleguide__section--depth-0 cui-styleguide__overview cui-styleguide__style">
            {{{homepage}}}
          </div>
        {{else}}

          {{!
            Display each section, in order.

            The "root" element comes first in this loop, and can be detected using
            the "#if @first" block as seen below.
          }}
          {{#each sections}}
            <{{#if @first}}div style="display:none"{{else}}section{{/if}} id="kssref-{{referenceURI}}" class="cui-tile cui-styleguide__section cui-styleguide__section--depth-{{depth}}">

              <div class="cui-styleguide__style">
                <h{{depth}} class="cui-tile__title cui-styleguide__title cui-styleguide__title--level-{{depth}} h4">
                  <a class="cui-styleguide__title__permalink" href="#kssref-{{referenceURI}}">
                    <span class="cui-styleguide__title__ref">
                      {{referenceNumber}}
                      
                      <span class="kss-title__permalink-hash">
                        {{!-- {{#if @root.hasNumericReferences}}
                          #permalink
                        {{else}}
                          #{{reference}}
                        {{/if}} --}}
                      </span>
                    </span>
                    {{header}}
                  </a>
                </h{{depth}}>

              </div>

              <div class="cui-tile__body">

                {{#if description}}
                  <div class="cui-styleguide__description-container">
                    <span class="cui-styleguide__description h4">DESCRIPTION: </span>{{{description}}}
                  </div>
                {{/if}}

                {{#if parameters}}
                  <div class="kss-parameters__title">Parameters:</div>
                  <ul class="cui-styleguide__parameters">
                  {{#each parameters}}
                    <li class="cui-styleguide__parameters__item">
                      <div class="cui-styleguide__parameters__name"><code>{{name}}</code></div>
                      <div class="cui-styleguide__parameters__description">
                        {{{description}}}
                        {{#if defaultValue}}
                          <div class="kss-parameters__default-value">
                            Defaults to: <code>{{defaultValue}}</code>
                          </div>
                        {{/if}}
                      </div>
                    </li>
                  {{/each}}
                  </ul>
                {{/if}}


              {{#if example}}
                <div class="cui-styleguide-modifier__wrapper" id="aoc">
                  <div class="cui-styleguide-modifier__heading cui-styleguide__style">
                    <div class="cui-styleguide-modifier__title h4">Example{{#if modifiers}}s{{/if}}</div>
                  </div>

                  {{#if modifiers}}
                    <div class="cui-styleguide-modifier__default-name cui-styleguide__style h5">
                      Default styling
                    </div>
                  {{/if}}

                  <div class="cui-styleguide__example">
                    {{{example}}}
                  </div>

                  {{#each modifiers}}
                    <div class="cui-styleguide-modifier__name cui-styleguide__style h5">
                      {{name}}
                    </div>
                    <div class="cui-styleguide-modifier__description cui-styleguide__style">
                      {{{description}}}
                    </div>
                    <div class="cui-styleguide-modifier__example">
                      {{{markup}}}
                    </div>
                  {{/each}}
                </div>
                {{#if example}}
                  <div class="cui-styleguide__markup-container cui-styleguide__style">
                    <pre class="cui-styleguide__markup prettyprint linenums lang-html"><code data-language="html">{{markup}}</code></pre>
                  </div>
                {{/if}}
              {{/if}}
              </div>

              {{!-- {{#if sourceFile.name}}
                <div class="kss-source kss-style">
                  Source: <code>{{sourceFile.name}}</code>, line {{sourceFile.line}}
                </div>
              {{/if}} --}}

            </{{#if @first}}div{{else}}section{{/if}}>
          {{/each}}
        {{/if}}
        </article>
      </div>
    </div>
  </snap-content>
</div>

<!-- SCRIPTS -->
<script src="kss-assets/kss.js"></script>
<script src="kss-assets/scrollspy.js"></script>
<script src="kss-assets/prettify.js"></script>
<script>
  prettyPrint();
  var spy = new ScrollSpy('#kss-node', {
    nav: '.kss-nav__menu-child > li > a',
    className: 'is-in-viewport'
  });
</script>
{{{scripts}}}

{{! This will only be included in the demo page. }}
{{#if options.demo}}
  <footer class="kss-github">
    <!-- https://github.com/blog/273-github-ribbons -->
    <a href="https://github.com/kss-node/kss-node"><img src="kss-assets/github-fork--black.png" alt="Fork me on GitHub"></a>
  </footer>
{{/if}}

<!-- Automatically built using <a href="https://github.com/kss-node/kss-node">kss-node</a>. -->
</body>
</html>
